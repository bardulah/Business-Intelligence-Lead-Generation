{
  "name": "lead-discovery-tool",
  "version": "2.0.0",
  "description": "Automated Lead Discovery and Research Tool - Production Ready",
  "main": "dist/server/index.js",
  "scripts": {
    "setup": "ts-node server/scripts/setup.ts",
    "dev": "concurrently \"npm run server:dev\" \"npm run worker:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon --exec ts-node server/index.ts",
    "worker:dev": "nodemon --exec ts-node server/scripts/start-worker.ts",
    "server:build": "tsc -p tsconfig.server.json",
    "client:dev": "cd client && npm run dev",
    "client:build": "cd client && npm run build",
    "build": "npm run server:build && npm run client:build",
    "start": "node dist/server/index.js",
    "start:worker": "node dist/server/scripts/start-worker.js",
    "start:all": "concurrently \"npm start\" \"npm run start:worker\"",
    "install-all": "npm install && cd client && npm install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset --force"
  },
  "keywords": [
    "lead-generation",
    "business-intelligence",
    "lead-discovery",
    "crm",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@sentry/node": "^7.91.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "email-validator": "^2.0.4",
    "dotenv": "^16.3.1",
    "pdfkit": "^0.14.0",
    "json2csv": "^6.0.0-alpha.2",
    "bull": "^4.12.0",
    "ioredis": "^5.3.2",
    "socket.io": "^4.6.0",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "zod": "^3.22.4",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "p-retry": "^6.1.0",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/pdfkit": "^0.13.4",
    "@types/compression": "^1.7.5",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "prisma": "^5.7.1",
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2",
    "@playwright/test": "^1.40.1",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
