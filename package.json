{
  "name": "lead-discovery-tool",
  "version": "1.0.0",
  "description": "Automated Lead Discovery and Research Tool",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "install-all": "npm install && cd client && npm install"
  },
  "keywords": [
    "lead-generation",
    "business-intelligence",
    "lead-discovery",
    "crm"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "whois-json": "^1.0.6",
    "email-validator": "^2.0.4",
    "node-cache": "^5.1.2",
    "dotenv": "^16.3.1",
    "pdfkit": "^0.14.0",
    "json2csv": "^6.0.0-alpha.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  }
}
